
<div class="contents row">
  <div class="container">
    <h3>投稿する</h3>
    <%= form_with(model: @rankings, local: true) do |form| %>
      <%= form.text_field :name, placeholder: "name" %>
      <%= form.text_field :image, placeholder: "Image Url" %>
      <span class="star-rating" data-rate="5"></span>
      <%= form.text_area :text, placeholder: "text", rows: "5" %>

      <%= form.submit "UP!" %>
    <% end %>
  </div>
</div>